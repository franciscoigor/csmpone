{
  "type": "object",
  "comment": "Login",
  "properties": {
    "username": {
      "type": "string",
      "label": "username",
      "update": {
        "type": "action",
        "action": [
          "submit"
        ]
      },
      "selector": "#UserName",
      "control": "text"
    },
    "password": {
      "type": "string",
      "label": "password",
      "update": {
        "type": "action",
        "action": [
          "submit"
        ]
      },
      "selector": "#Password",
      "control": "text"
    },
    "errorMessage": {
      "type": "string",
      "label": "errorMessage",
      "update": {
        "type": "none"
      },
      "selector": "#UserName",
      "control": "property.innerText"
    },
    "submit": {
      "type": "action",
      "parameters": {
        "username": {
          "type": "string"
        },
        "password": {
          "type": "string"
        }
      },
      "selector": ".btn.btn-primary.form-control",
      "control": "action.button"
    },
    "userErrorMessage": {
      "type": "string",
      "update": {
        "type": "none"
      },
      "selector": ".field-validation-error[data-valmsg-for=\"UserName\"]",
      "control": "property.innerText"
    },
    "passwordErrorMessage": {
      "type": "string",
      "update": {
        "type": "none"
      },
      "selector": ".field-validation-error[data-valmsg-for=\"Password\"]",
      "control": "property.innerText"
    },
    "isLogin": {
      "type": "identifier",
      "selector": "title",
      "control": "exists",
      "selectContains": "CSMP Login"
    }
  },
  "connect": []
}
